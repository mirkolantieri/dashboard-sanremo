{
    "name": "jsdoc_config",
    "main": "index.js",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "better-docs": {
        "cleverLinks": false,
        "monospaceLinks": false
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }


}